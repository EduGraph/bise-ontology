###  http://akwi.de/ns/bise#BISEJobProfile

bise:BISEJobProfile  	a			        	owl:Class ;
						rdfs:label				"BISEJobProfile" ;
						rdfs:subClassOf			schema:QualitativeValue ;
                        skos:prefLabel         	"Tätigkeitsprofil"@de ;
                        skos:prefLabel			"job profile"@en ;
