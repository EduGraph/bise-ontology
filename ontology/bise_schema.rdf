        <rdfs:comment xml:lang="de">Dreiwertiger Vektor (Triple), der die Anteile eines Studiengangs (Curriculums) an den drei Säulen der Wirtschaftsinformatik ausweist.</rdfs:comment>
        <skos:prefLabel xml:lang="de">Anteile der WI-Säulen am Curriculum</skos:prefLabel>
        <skos:prefLabel xml:lang="en">ise pillar shares with respect to curriculum</skos:prefLabel>
    </owl:Class>
    


    <!-- http://akwi.de/ns/bise#CSCMasterFocusBISE -->

    <owl:Class rdf:about="&bise;CSCMasterFocusBISE">
        <rdfs:label>CSCMasterFocusBISE</rdfs:label>
        <rdfs:subClassOf rdf:resource="&bise;DegreeProgram"/>
